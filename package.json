    "url": "https://github.com/nens/lizard-management-client.git"
  },
  "keywords": [
    "lizard",
    "water",
    "management"
  ],
  "author": "Gijs Nijholt",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/nens/lizard-management-client/issues"
  },
  "homepage": "/management",
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jquery": "^3.2.1",
    "leaflet": "^1.2.0",
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "lodash.find": "^4.6.0",
    "mime": "1.4.0",
    "mime-lookup": "^0.0.2",
    "node-mime": "^1.0.0",
    "object-assign": "4.1.1",
    "pluralize": "^7.0.0",
    "pondjs": "^0.8.7",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "16.3.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-transition-group": "^15.6.2",
    "react-custom-scrollbars": "^4.1.2",
    "react-dev-utils": "^4.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "16.3.0",
    "react-ink": "^6.2.0",
    "react-intl": "^2.4.0",
    "react-leaflet": "^1.7.0",
    "react-md-spinner": "^0.2.5",
    "react-onclickoutside": "^6.6.0",
    "react-pose": "^1.5.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-select": "1.0.0-rc.10",
    "react-timeseries-charts": "^0.12.8",
    "react-toolbox": "^2.0.0-beta.12",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "underscore": "^1.8.3",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "json-format": "^1.0.1",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.3",
    "react-hot-loader": "next",
    "react-intl-translations-manager": "^5.0.1",
    "release-it": "7.3.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && scripts/makedist.sh",
    "test": "node scripts/test.js --env=jsdom",
    "release": "release-it --verbose",
    "staging-deploy": "read -p 'Version? (see above) ' V && test $V && ansible-playbook -i deploy/hosts -K deploy/deploy.yml --limit=staging --extra-vars=\"sitename=staging.nxt.lizard.net version=$V\"",
    "production-deploy": "read -p 'Version? (see above) ' V && test $V && ansible-playbook -i deploy/hosts -K deploy/deploy.yml --limit=production --extra-vars=\"sitename=nxt.lizard.net version=$V\"",
    "precommit": "lint-staged",
    "i18n:update": "node translationRunner.js",
    "i18n:extract": "node extract.js",
    "i18n:extract-then-update": "node extract.js && node translationRunner.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.jsx": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": {
    "/bootstrap/lizard/": {
      "target": "https://demo.lizard.net/",
      "changeOrigin": true,
      "ssl": false,
      "secure": false,
      "headers": {
        "username": "",
        "password": ""
      }
    },
    "/api/v3/": {
      "target": "https://demo.lizard.net/",
      "changeOrigin": true,
      "ssl": false,
      "secure": false,
      "headers": {
        "username": "",
        "password": ""
      }
    }
  }
}
